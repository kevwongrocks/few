<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kevwong.few"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="13" android:targetSdkVersion="19" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" 
        android:theme="@android:style/Theme.Holo.Light" 
        android:uiOptions="none">
        <activity
            android:name=".ViewPagerActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayResult"
            android:parentActivityName=".ViewPagerActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kevwong.few.ViewPagerActivity" />
        </activity>
    </application>

</manifest>